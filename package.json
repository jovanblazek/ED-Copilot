{
  "name": "ED-Copilot",
  "version": "2.2.1",
  "description": "Elite Dangerous Discord bot",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/index.ts",
    "translations:watch": "typesafe-i18n",
    "test": "jest",
    "register-commands": "ts-node -T src/utils/registerCommands.ts",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "generate-types": "prisma generate",
    "build": "pnpm run generate-types && rimraf build/ && tsc --project tsconfig.production.json && pnpm run postbuild",
    "postbuild": "copyfiles .env ./build",
    "start": "node build/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jovanblazek/ed-copilot.git"
  },
  "author": "jovanblazek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jovanblazek/ED-Copilot/issues"
  },
  "homepage": "https://github.com/jovanblazek/ED-Copilot#readme",
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "@sentry/node": "^8.11.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "discord.js": "14.15.3",
    "dotenv": "^16.4.5",
    "got": "^11.8.6",
    "jsdom": "^21.1.0",
    "koa": "^2.15.3",
    "lodash": "^4.17.21",
    "make-error": "^1.3.6",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "socket.io-client": "^4.7.5",
    "typesafe-i18n": "^5.26.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/koa": "^2.15.0",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "prisma": "^5.15.1",
    "rimraf": "^5.0.7",
    "semantic-release": "^24.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": "^20"
  },
  "packageManager": "pnpm@9.4.0",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "./CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ]
        }
      ]
    ],
    "preset": "conventionalcommits",
    "presetConfig": {
      "types": [
        {
          "type": "feat",
          "section": ":sparkles: Features",
          "hidden": false
        },
        {
          "type": "fix",
          "section": ":bug: Bug Fixes",
          "hidden": false
        },
        {
          "type": "enh",
          "section": ":up: Enhancements",
          "hidden": false
        },
        {
          "type": "revert",
          "section": ":rewind: Reverts",
          "hidden": false
        },
        {
          "type": "docs",
          "section": ":books: Documentation",
          "hidden": false
        },
        {
          "type": "chore",
          "section": ":octopus: Miscellaneous Chores",
          "hidden": false
        },
        {
          "type": "test",
          "section": ":white_check_mark: Tests",
          "hidden": false
        }
      ]
    }
  }
}
